class ExpressionSentence{
public static void main(String[] args) {
int k, i=3, j=4;
k=20*8/4+i+j*i; System.out.println("表达式（20*8/4+i+j*i）＝"+k);
}
}

class CompoundSentence{
public static void main(String args[]) {
int k, i=3, j=4;
k=i+j;
System.out.println("在复合块外的输出k="+k);
